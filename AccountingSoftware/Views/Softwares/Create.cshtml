@model AccountingSoftware.Models.Software

@{
    ViewData["Title"] = "Создание ПО";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Создание ПО</h1>

<h4>Software</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="SoftwareTechnicalDetailsId" class="control-label"></label>
                <select asp-for="SoftwareTechnicalDetailsId" class ="form-control" asp-items="ViewBag.SoftwareTechnicalDetailsId"></select>
            </div>
            <div class="form-group">
                <label asp-for="LicenceId" class="control-label"></label>
                <select asp-for="LicenceId" class ="form-control" asp-items="ViewBag.LicenceId"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Вернутсья к списку</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
